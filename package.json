{
  "name": "servicerequestapi",
  "version": "0.0.0",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "node index.js --watch",
    "serve": "nodemon index.js",
    "nodemon:build": "nodemon --watch server/ --exec babel ./server --out-dir ./dist --source-maps --copy-files",
    "start:debug": "cross-env DEBUG=servicerequestapi:* yarn start",
    "lint": "esw *.js server config --color --fix",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/titansales/servicerequestapi.git"
  },
  "homepage": "https://gitlab.com/titansales/servicerequestapi#README",
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "africastalking": "^0.4.5",
    "apn": "^2.2.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "composable-middleware": "0.3.0",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "core-js": "^3.6.5",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "fcm-node": "^1.5.2",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "interswitch": "^2.1.7",
    "jade": "~1.11.0",
    "joi": "10.6.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "^4.17.15",
    "method-override": "^2.3.10",
    "mongoose": "4.7.4",
    "mongoose-sequence": "5.2.2",
    "morgan": "1.9.0",
    "node-cron": "2.0.3",
    "nodemailer": "6.4.2",
    "paystack": "false2.0.1",
    "paystack-node": "^0.2.4",
    "qr-image": "^3.2.0",
    "qrcode": "false1.4.4",
    "shortid": "2.2.15",
    "socket.io": "^1.7.4",
    "socket.io-client": "^1.5.1",
    "socketio-jwt": "^4.5.0",
    "swagger-jsdoc": "3.5.0",
    "swagger-ui-express": "4.1.2",
    "underscore": "^1.9.1",
    "uniqid": "^5.2.0",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "express-swagger-generator": "^1.1.15",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-uglify": "3.0.2",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "nodemon": "2.0.2",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
